<?xml version="1.0" encoding="utf-8"?>
<MyEffectButtonImpl horizontalScrollPolicy="off" verticalScrollPolicy="off" xmlns="com.glearning.melee.components.player.*" xmlns:mx="http://www.adobe.com/2006/mxml" width="70" height="70">
   <mx:Fade id="fadeOut" duration="300" alphaFrom="1.0" alphaTo="0.0"/>
   <mx:Fade id="fadeIn" duration="300" alphaFrom="0.0" alphaTo="1.0"/>
   <mx:Zoom id="zoomAll" zoomWidthTo="2" zoomHeightTo="2" zoomWidthFrom="1" zoomHeightFrom="1"  />
	
   <mx:Image id="image" x="10" y="10" width="48" height="49" hideEffect="{fadeOut}" showEffect="{fadeIn}" visible="false" />
   <mx:LinkButton x="10" y="10" height="49" width="48" click="doZoom(event)" id="campName"/>
	
</MyEffectButtonImpl>
